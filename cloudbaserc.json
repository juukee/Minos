{
    "version": "2.0",
    "envId": "{{envId}}",
    "$schema": "https://framework-1258016615.tcloudbaseapp.com/schema/latest.json",
    "framework": {
      "name": "minos",
      "plugins": {
        "node": {
          "use": "@cloudbase/framework-plugin-node",
          "inputs": {
            "name": "minos",
            "path": "/",
            "entry": "app.js",
            "buildCommand": "npm install --prefer-offline --no-audit --progress=false && npm run build",
            "ignore": ".vscode",
            "functionOptions": {
              "timeout": 5,
              "envVariables": {
                "NODE_ENV": "production"
              }
            }
          }
        }
      }
    }
  }